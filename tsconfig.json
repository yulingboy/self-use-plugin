{
  "extends": "plasmo/templates/tsconfig.base",
  "jsx": "react",
  "exclude": [
    "node_modules"
  ],
  "include": [
    "types/*.d.ts",
    ".plasmo/index.d.ts",
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "compilerOptions": {
    "paths": {
      "@/*": [ "./src/*"],
      "@newtab/*": ["./src/newtab/*"],
      "@utils/*": ["./src/utils/*"],
    },
    "baseUrl": "."
  }
}
